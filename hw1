import java.util.Random;
import java.util.Scanner;

public class RandomNumberYourLastName {

	public static void main(String[] args) {
		
		int num;
		int lowestNum,highestNum;
		int randomNum;
		
		Scanner sc=new Scanner(System.in);
		
		System.out.print("How many random number do you want to see: ");
		num=sc.nextInt();

		System.out.print("Smallest number: ");
		lowestNum=sc.nextInt();
		
		System.out.print("Largest number: ");
		highestNum=sc.nextInt();
		
		Random rand= new Random();
		
		for(int i=1;i<=num;i++)
		{ 	
			randomNum=rand.nextInt(highestNum)+lowestNum;
			System.out.println(randomNum);
		}
	}

}
